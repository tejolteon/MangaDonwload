<Window x:Class="MDR.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MDR"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="grid">
        <Image x:Name="imageLeft" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center"/>
        <Image x:Name="imageRight" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center"/>

    </Grid>
</Window>
